{
    "id": "000",
    "name": "vnf-br-000",
    "version": "0.1",
    "author": "VNF Benchmark Report",
    "description": "Realizes the test of two agents ping on localhost",
    "vnf": {
        "id": "",
        "name": "",
        "version": "",
        "author": "",
        "description": "No SUT VNF."
    },
    "environment": {
        "name": "Execution-Environment",
        "description": "Esablishes scenario in containernet",
        "deploy": true,
        "orchestrator": {
            "type": "ssh",
            "description": "Uses containernet to deploy vnfbd scenario",
            "parameters": {
                "address": {
                    "input": "address",
                    "value": "172.17.0.1:9090"
                }
            }
        }
    },
    "inputs": {
        "variables": {},
        "vnfbd": {
            "experiments": {
                "trials": 1,
                "tests": 1
            },
            "scenario": {
                "nodes": {
                    "d1": {
                        "id": "d1",
                        "type": "internal",
                        "image": "raphaelvrosa/gym:latest",
                        "format": "process",
                        "role": "agent",
                        "resources": {},
                        "connection_points": {},
                        "relationships": {},
                        "lifecycle": {
                            "start": {
                                "workflow": "start",
                                "name": "",
                                "parameters": {
                                    "host_id": {
                                        "input": "host_id",
                                        "value": "d1"
                                    },
                                    "host_ip": {
                                        "input": "host_ip",
                                        "value": "get_attrib:ip"
                                    }
                                },
                                "implementation": [
                                    "gym-agent --uuid {host_id} --address {host_ip}:50055 > gym-agent.log 2>&1 &"
                                ]
                            }
                        }
                    },
                    "d2": {
                        "id": "d2",
                        "type": "internal",
                        "image": "raphaelvrosa/gym:latest",
                        "format": "process",
                        "role": "agent",
                        "resources": {},
                        "connection_points": {},
                        "relationships": {},
                        "lifecycle": {
                            "start": {
                                "workflow": "start",
                                "name": "",
                                "parameters": {
                                    "host_id": {
                                        "input": "host_id",
                                        "value": "d2"
                                    },
                                    "host_ip": {
                                        "input": "host_ip",
                                        "value": "get_attrib:ip"
                                    }
                                },
                                "implementation": [
                                    "gym-agent --uuid {host_id} --address {host_ip}:50055 > gym-agent.log 2>&1 &"
                                ]
                            }
                        }
                    },
                    "d3": {
                        "id": "d3",
                        "type": "internal",
                        "image": "raphaelvrosa/gym:latest",
                        "format": "process",
                        "role": "manager",
                        "resources": {},
                        "connection_points": {},
                        "relationships": {},
                        "lifecycle": {
                            "start": {
                                "workflow": "start",
                                "name": "",
                                "parameters": {
                                    "host_id": {
                                        "input": "host_id",
                                        "value": "d3"
                                    },
                                    "host_ip": {
                                        "input": "host_ip",
                                        "value": "get_attrib:ip"
                                    }
                                },
                                "implementation": [
                                    "gym-manager --uuid {host_id} --address {host_ip}:50055 > gym-manager.log 2>&1 &"
                                ]
                            }
                        }
                    }                    
                },
                "links": {},
                "policies": {}
            },
            "proceedings": {
                "agents": {
                    "1": {
                        "uuid": "d1",
                        "name": "",
                        "probers": {
                            "2": {
                                "id": 2,
                                "instances": 2,
                                "name": "ping",
                                "parameters": {
                                    "interval": {
                                        "input": "interval",
                                        "value": "1"
                                    },
                                    "packets": {
                                        "input": "packets",
                                        "value": "5"
                                    },
                                    "frame_size": {
                                        "input": "frame_size",
                                        "value": "64"
                                    },
                                    "target": {
                                        "input": "target",
                                        "value": "www.google.com"
                                    }
                                }
                            },
                            "4": {
                                "id": 4,
                                "instances": 1,
                                "name": "iperf3",
                                "parameters": {
                                    "client": {
                                        "input": "client",
                                        "value": "True"
                                    },
                                    "server": {
                                        "input": "server",
                                        "value": "127.0.0.1"
                                    },
                                    "port": {
                                        "input": "port",
                                        "value": "6594"
                                    },
                                    "protocol": {
                                        "input": "protocol",
                                        "value": "udp"
                                    },
                                    "duration": {
                                        "input": "duration",
                                        "value": "10"
                                    },
                                    "rate": {
                                        "input": "rate",
                                        "value": "10G"
                                    }
                                }
                            }
                        }
                    },
                    "2": {
                        "uuid": "d2",
                        "name": "",
                        "probers": {
                            "2": {
                                "id": 2,
                                "instances": 2,
                                "name": "ping",
                                "parameters": {
                                    "interval": {
                                        "input": "interval",
                                        "value": "1"
                                    },
                                    "packets": {
                                        "input": "packets",
                                        "value": "5"
                                    },
                                    "frame_size": {
                                        "input": "frame_size",
                                        "value": "128"
                                    },
                                    "target": {
                                        "input": "target",
                                        "value": "127.0.0.1"
                                    }
                                }
                            },
                            "4": {
                                "id": 4,
                                "instances": 1,
                                "name": "iperf3",
                                "parameters": {
                                    "client": {
                                        "input": "client",
                                        "value": "False"
                                    },
                                    "port": {
                                        "input": "port",
                                        "value": "6594"
                                    },
                                    "duration": {
                                        "input": "duration",
                                        "value": "10"
                                    }
                                }
                            }
                        }
                    }
                },
                "monitors": {}
            }
        }
    },
    "outputs": {}
}
